@startuml
!include style.puml
skinparam ArrowFontStyle plain

box Model MODEL_COLOR_T1
participant ":Model" as Model MODEL_COLOR
participant ":VersionedBloodNet" as VersionedBloodNet MODEL_COLOR
end box

[-> Model : undoBloodNet()
activate Model

Model -> VersionedBloodNet : undo()
activate VersionedBloodNet

VersionedBloodNet -> VersionedBloodNet :resetData(ReadOnlyBloodNet)
VersionedBloodNet --> Model :
deactivate VersionedBloodNet

[<-- Model
deactivate Model

@enduml
